{
  "name": "vue-final-validate",
  "version": "1.0.4",
  "description": "Vue validation solution from my development experience.",
  "keywords": [
    "Vue",
    "Vue2",
    "Validator",
    "Vue validate"
  ],
  "main": "dist/vue-final-validate.cjs.js",
  "module": "dist/vue-final-validate.esm.js",
  "author": "phphe <phphe@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phphe/vue-final-validate.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vue-cli-service serve",
    "build-vue": "vue-cli-service build",
    "build": "bili --format cjs --format umd  --format umd-min --format esm --banner",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "helper-js": "^1.4.6",
    "vue": "^2.5.21",
    "vue-functions": "^1.0.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "bili": "^4.8.1",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
